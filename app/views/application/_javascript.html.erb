<%= javascript_include_tag "https://js.stripe.com/v2/" %>
<%= javascript_tag do %>
 Stripe.setPublishableKey('<%= ENV['STRIPE_PUBLISHABLE_KEY'] %>');
<% end %>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => 'reload' %>
<% if Rails.env.test? %>
  <%= javascript_tag do %>
   $.fx.off = true;
   $.ajaxSetup({ async: false });
  <% end %>
<% end %>
