<tr>
  <td><%= card.brand %></td>
  <td>XXXX-XXXX-XXXX-<%= card.last4 %></td>
  <td><%= '%.2d' % card.exp_month %>/<%= card.exp_year %></td>
  <td>
    <% if card.default? %>
      <span class="card-default">Default</span>
    <% end %>
  </td>
  <td><%= link_to embedded_svg("trashcan.svg"), account_card_path(card), method: :delete, data: { confirm: "Are you are you want to delete this card?" } %></td>
</tr>
