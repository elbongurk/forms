<tr data-url="<%= form_path(form) %>">
  <td><%= local_time(form.created_at) %></td>
  <td><%= form.name %></td>
  <td><%= form.submissions.size %></td>
  <td><%= local_time(form.updated_at) %></td>
  <td><%= link_to "Edit", edit_form_path(form) %></td>
  <td><%= link_to "Destroy", form_path(form), method: :delete, data: { confirm: "Are you sure you want to delete this form?" } %></td>
</tr>
