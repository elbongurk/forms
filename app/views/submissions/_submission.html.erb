<tr data-url="<%= form_submission_path(submission.form, submission) %>">
  <td><%= local_time(submission.created_at) %></td>
  <td><%= submission.preview %></td>
  <td><%= link_to "Destroy", form_submission_path(submission.form, submission), method: :delete, data: { confirm: "Are you sure you want to delete this submission?" } %></td>
</tr>
